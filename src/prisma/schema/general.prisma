// more like dynamic enums

model Designation {
  designation String    @id
  Faculty     Faculty[]
}

model Department {
  department String      @id
  faculties  Faculty[]
  infraItems InfraItem[]
}

model Batch {
  batch    String    @id
  students Student[]
}

model Branch {
  branch   String    @id
  students Student[]
}

model Group {
  group    String    @id
  students Student[]
}

model StaffType {
  staffType String  @id
  Staff     Staff[]
}
